sigma_prime <- function(z) {
sigma_z <- sigma(z)
(1 - sigma_z) * sigma_z
}
#Finding the derivative using numerical approximation
h <- 0.0001
approx_derivative <- function(z) {
(sigma(z + h) - sigma(z - h)) / (2 * h)
}
all.equal(sigma_prime(z), approx_derivative(z), check.attributes = FALSE)
